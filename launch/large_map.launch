<?xml version="1.0"?>
<launch>
    <node pkg="explore_large_map" type="explore_large_map_node" name="explore_large_map_node" output="screen" >
        <param name="map_width" type="double" value="1000"/>
        <param name="map_height" type="double" value="1000" />
        <param name="map_resolution" type="double" value="0.1" />
    </node>

    <node pkg="explore_large_map" type="local_map_node" name="local_map_node" output="screen" >
        <param name="map_width" type="double" value="500"/>
        <param name="map_height" type="double" value="1250" />
        <param name="map_resolution" type="double" value="0.1" />
    </node>

    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find explore_large_map)/cfg/explore_map_viz.rviz"/>

</launch>
